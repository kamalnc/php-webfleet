{
	"name": "codemasher/php-webfleet",
	"description": "A wrapper for the TomTom Business Webfleet.connect API. PHP 5.6+, PHP 7",
	"homepage": "https://github.com/codemasher/php-webfleet",
	"license": "MIT",
	"type": "library",
	"keywords": [
		"webfleet", "tomtom", "telematics" ,"api", "wrapper"
	],
	"authors": [
		{
			"name": "Smiley",
			"email": "smiley@chillerlan.net"
		}
	],
	"require": {
		"php": ">=5.6.0",
		"guzzlehttp/guzzle": "~6.0",
		"vlucas/phpdotenv": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "5.1.*"
	},
	"autoload": {
		"psr-4": {
			"TomTom\\Telematics\\": "src"
		}
	},
	"autoload-dev": {
		"classmap": [ "tests/" ]
	}
}
